#!/bin/bash
#cp ~/PS/Tool/default.cpp ~/PS/Tool/labtemp.cpp
if [ "$2" = "-p" ]; then
cp "$1" ~/PS/Tool/templab.py
vi ~/PS/Tool/templab.py
cp ~/PS/Tool/templab.py ~/PS/Tool/templab.py.bak
fi
if [ "$2" = "-c" ]; then
cp "$1" ~/PS/Tool/templab.cpp
vi ~/PS/Tool/templab.cpp
cp ~/PS/Tool/templab.cpp ~/PS/Tool/templab.cpp.bak
fi
