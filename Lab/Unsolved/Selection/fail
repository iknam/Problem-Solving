#include <iostream>
#include <numeric>
#include <vector>
#include <algorithm>

using namespace std;

// iota not support

int main()
{
	vector<int> Numberlist;

	Numberlist.push_back( 2 );

	Numberlist.push_back( 5 );

	Numberlist.push_back( 7 );

	iota( Numberlist.begin(), Numberlist.end(), 5 );



	for( auto IterPos = Numberlist.begin(); IterPos != Numberlist.end(); ++IterPos )

	{

		cout << *IterPos << endl;

	}

    return 0;
}

