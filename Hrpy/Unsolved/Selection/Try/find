from operator import itemgetter

dic = {}

n = int(input())

for i in range(0, n):
    name = str(input())
    score1 = int(input())
    score2 = int(input())
    score3 = int(input())
    average = (score1 + score2 + score3) // 3
    dic[name] = average
    #print("{:.2f}".format(dic['average']))
    #sorted(dic.iteritems(), key = itemgetter(1), reverse=True)
if name == str(input()):
    print(dic[name])
